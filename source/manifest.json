{
	"manifest_version": 2,
	"name": "Test Chrome Extension",
	"description": "Test Chrome Extension",
  "version": "1.0.0",
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches":["https://---------------/*"],
      "js":["js/all.js"],
      "run_at":"document_idle"
    }
  ],
	"permissions": [
    "activeTab",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
